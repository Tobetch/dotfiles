{
  "name": "sample",
  "description": "sample",
  "version": "1.0.0",
  "author": "sample",
  "private": true,
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "upgrade": "if [[ $(npm list -g npm-check-updates | grep 'empty' > /dev/null; echo $? ) != 0 ]]; then ncu -a && npm i; else echo 'Installing NCU ...'; npm install -g npm-check-updates && npm run upgrade; fi",
    "start": "SET_ENV=development ./toooooools/deployments/dev/start.sh dev --skip-compression",
    "start:compress": "SET_ENV=development ./toooooools/deployments/dev/start.sh dev",
    "build": "NODE_ENV='production' NODE_OPTIONS=--max_old_space_size=4096 gulp prod",
    "build:dev": "NODE_ENV=development API_HOST=https://sample.com NODE_OPTIONS=--max_old_space_size=4096 gulp dev",
    "build:docker": "run(){ ./deployments/local/build.sh $1; }; run",
    "analyze": "BUNDLE_ANALYZE=browser npm run build",
    "serve": "NODE_ENV='production' pm2 start ./app/server.js -i max --name 'sample'",
    "serve:dev": "NODE_ENV=development node --inspect ./app/server.js",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "aigis": "aigis run -c ./aigis_config.yml && gulp assets:aigis && http-server ./toooooools/aigis/styleguide",
    "api": "nodemon -e js ./api/server.js",
    "reset": "npm i npm@latest -g; sudo rm -rf node_modules && npm ci",
    "reset:hard": "npm i npm@latest -g && rm package-lock.json; sudo rm -rf node_modules && npm i",
    "tag": "bump --prompt --commit --tag --push",
    "deploy": "run(){ ./toooooools/travis/deployments/deploy.sh $1 $2; }; run",
    "deploy:docker": "run(){ ./deployments/agkbtest/deploy.sh $1; }; run",
    "test": "./tests/runner.sh",
    "test:visual": "NODE_ENV=development gulp test:visual",
    "eslint": "eslint --cache",
    "eslint:all": "eslint --cache './**'",
    "unit": "run() { NODE_ENV=test node_modules/jest/bin/jest.js $1 --coverage --config=./app/jest.config.js; }; run",
    "unit:single": "npm run unit -t",
    "unit:update": "NODE_ENV=test node_modules/jest/bin/jest.js --updateSnapshot --config=./app/jest.config.js",
    "storybook": "NODE_ENV=development gulp start:storybook",
    "storybook:noopen": "NODE_ENV=development gulp start:storybook:noBrowser",
    "storybook:toc": "NODE_ENV=development gulp build:storybook:toc"
  },
  "browserslist": [
    "last 2 versions"
